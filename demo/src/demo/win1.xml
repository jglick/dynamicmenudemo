<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Build">
            <file name="demo-Win1Action.instance">
                <attr name="displayName" stringvalue="Action for Window #1"/>
                <attr methodvalue="org.openide.awt.Actions.alwaysEnabled" name="instanceCreate"/>
                <attr name="delegate" newvalue="demo.Win1Action"/>
                <attr boolvalue="false" name="noIconInMenu"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="First">
            <attr name="position" intvalue="2000"/>
            <file name="demo-Win1Action.shadow">
                <attr name="originalFile" stringvalue="Actions/Build/demo-Win1Action.instance"/>
                <attr intvalue="0" name="position"/>
            </file>
        </folder>
    </folder>
</filesystem>
